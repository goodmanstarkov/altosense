{
  "name": "altosenso",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint:fix": "eslint . --ext .ts,.tsx, --color --fix",
    "prettier:fix": "prettier --write **/*.{ts,tsx,json}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run prettier:fix",
      "npm run lint:fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.40.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@next/font": "^13.0.2",
    "core-js": "^3.25.5",
    "next": "^13.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
